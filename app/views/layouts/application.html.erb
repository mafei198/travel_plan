<!DOCTYPE html>
<html>
  <head>
    <title>TravelPlan</title>
    <%= stylesheet_link_tag :all %>

  <%= javascript_include_tag 'jquery-1.5.1.min.js', 'jquery-ui-1.8.13.custom.min.js', 'autocomplete-rails.js', 'rails.js', 'jquery.datePicker.js','date.js', 'rails.validations.js' %>
    <%= csrf_meta_tag %>
    <script type='text/javascript'>
      $(function(){
          $("#plan_valid_date").datePicker();
          });
    </script>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div id="container" class="plan" style="background:#FFFFFF;">
      <%= yield %>
      <div class="clear"></div>
    </div>
    <%= render 'layouts/footer' %>
  </body>
</html>
